/* Tooltip Derecha */

.ask > ul {
  visibility:hidden;
  opacity:0;
  transition: visibility 0s linear 0.5s,opacity 0.5s linear;
  background:white;
  font-size:10px;
  padding:10px;
  display:block;
  border-radius:10px;
  display:block;
  width:255px;
  position:absolute;
  margin-left:140px;
  margin-top:-220px;
  color:#eee;
  height:190px;
  background:url("../images/fondo_over_flecha.png") no-repeat left;
  overflow:visible !important;
}

.ask:hover > ul {
  visibility:visible;
  opacity:1;
    overflow:visible !important;
	z-index:200!important;
		 position:absolute;
}

.ask li {
		 position:absolute;
    list-style-type:none;
    background:white;
      overflow:visible !important;
	z-index:200!important;
	min-width:215px;
	min-height:190px;
}
    
.ask li h1 { color:#FFFFFF; margin:0px; }
.ask li h2 { color:FFFFFF; font-weight:normal; margin:0px;  }
.ask li p { color:#333; font-size:12px;}



.ask_small > ul {
  visibility:hidden;
  opacity:0;
  transition: visibility 0s linear 0.5s,opacity 0.5s linear;
  background:white;
  font-size:10px;
  padding:10px;
  display:block;
  border-radius:10px;
  display:block;
  width:100px;
  position:absolute;
  margin-left:140px;
  margin-top:-220px;
  color:#eee;
  height:190px;
  background:url("../images/fondo_over_flecha.png") no-repeat left;
}

.ask_small:hover > ul {
  visibility:visible;
  opacity:1;
  z-index:1;
}

.ask_small li {
    list-style-type:none;
    padding:10px;
    padding-left:10px;
    padding-right:10px;
    background:white;
    }
    

